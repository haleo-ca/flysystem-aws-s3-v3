{
    "name": "league/flysystem-aws-s3-private",
    "description": "Extend flysystem adapter for the AWS S3 SDK and remove public option",
    "license": "MIT",
    "authors": [
        {
            "name": "Elie Sauveterre",
            "email": "elie.sauveterre@haleo.ca"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "league/flysystem": "^1.0.40",
        "aws/aws-sdk-php": "^3.0.0",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.0.0",
        "henrikbjorn/phpspec-code-coverage" : "~1.0.1"
    },
    "autoload": {
        "psr-4": {
            "League\\Flysystem\\AwsS3Private\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
